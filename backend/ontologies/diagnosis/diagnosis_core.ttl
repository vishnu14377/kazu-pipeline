@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix : <http://example.org/diagnosis-ontology#> .

:DiagnosisAssertion rdf:type owl:Class ;
    rdfs:label "Diagnosis Assertion"@en .

:diagnoses rdf:type owl:ObjectProperty ;
    rdfs:label "diagnoses"@en ;
    rdfs:domain :DiagnosisAssertion ;
    rdfs:range <http://example.org/disease-ontology#Disease> .

:has_entry_criterion rdf:type owl:ObjectProperty ;
    rdfs:label "has entry criterion"@en ;
    rdfs:domain <http://example.org/main-ontology#Cohort_Definition> ;
    rdfs:range :DiagnosisAssertion .
